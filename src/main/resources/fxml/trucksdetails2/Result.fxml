<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.po.constraintprogrammingsolver.gui.controllers.truckdetailscontrollers2.ResultController">
   <children>
      <TableView fx:id="packageLocationTable" editable="false" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="vehicleColLocation" prefWidth="75.0" text="Vehicle" />
          <TableColumn fx:id="packagesCol" prefWidth="75.0" text="Packages" />
        </columns>
         <HBox.margin>
            <Insets bottom="3.0" left="10.0" right="10.0" top="3.0" />
         </HBox.margin>
      </TableView>
      <TableView fx:id="vehiclesLoadTable" editable="false" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="vehicleColLoad" prefWidth="75.0" text="Vehicle" />
          <TableColumn fx:id="loadCol" prefWidth="75.0" text="Load" />
        </columns>
         <HBox.margin>
            <Insets bottom="3.0" right="10.0" top="3.0" />
         </HBox.margin>
      </TableView>
      <Label text="Cost:">
         <HBox.margin>
            <Insets right="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
       <Text fx:id="costText">
           <HBox.margin>
               <Insets top="10.0" />
           </HBox.margin>
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
       </Text>
   </children>
</HBox>
